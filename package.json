{
  "name": "@omniex/node-quickfix",
  "version": "2.1.114",
  "scripts": {
    "configure": "node-gyp configure",
    "install": "node-pre-gyp install --fallback-to-build",
    "postinstall": "node scripts/npm-run/postinstall.js",
    "test": "mocha"
  },
  "main": "index.js",
  "description": "This is a fork of node-quickfix, a wrapper for the Quickfix C++ library to be used within node.js applications",
  "author": {
    "name": "Kermen Deol",
    "email": "kermen.deol@electronifie.com"
  },
  "contributors": [
    {
      "name": "Nestor Toro",
      "email": "npm@omniex.io"
    }
  ],
  "devDependencies": {
    "dateformat": "^3.0.2",
    "mocha": "^5.2.0",
    "nan": "^2.8.0",
    "node-gyp": "3.8.0",
    "should": "^13.2.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/omni3x/node-quickfix"
  },
  "dependencies": {
    "bindings": "^1.3.0",
    "node-pre-gyp": "0.9.0"
  },
  "bundledDependencies": [
    "node-pre-gyp"
  ],
  "binary": {
    "module_name": "NodeQuickfix",
    "module_path": "./bindings/{platform}-{arch}",
    "host": ""
  },
  "files": [
    "index.js",
    "bindings",
    "lib",
    "scripts/npm-run"
  ]
}
